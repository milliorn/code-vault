{
  "name": "travel-journal",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://milliorn.github.io/Travel-Journal",
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-scripts": "5.0.1",
    "react-uuid": "2.0.0",
    "typescript": "5.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css-minify": "npx tailwindcss -o ./build/build.css --minify",
    "css-update": "npm install -D tailwindcss@latest postcss@latest autoprefixer@latest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "22.5.2",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "gh-pages": "6.1.1",
    "postcss": "8.4.45",
    "tailwindcss": "3.4.10"
  }
}
